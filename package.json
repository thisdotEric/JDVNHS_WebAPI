{
    "name": "jdvnhs-webapi",
    "version": "1.0.0",
    "description": "Jose de Villa National High School Web API",
    "main": "server.ts",
    "engines": {
        "node": "16.x",
        "npm": "8.x"
    },
    "scripts": {
        "dev": "tsnd --respawn --transpile-only src/server.ts",
        "pretty": "npx prettier --write .",
        "test": "jest",
        "build": "rm -rf dist &&  tsc -p .",
        "start": "node dist/server.js",
        "knex": "npx knex --knexfile src/database/knexQueryBuilder/knexfile.ts",
        "prod:knex": "knex --knexfile dist/src/database/knexQueryBuilder/knexfile.js"
    },
    "keywords": [],
    "author": "John Eric Siguenza",
    "license": "ISC",
    "dependencies": {
        "connect-session-knex": "^2.1.1",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "faker": "^5.5.3",
        "inversify": "^6.0.1",
        "inversify-express-utils": "^6.4.3",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.21.21",
        "pg": "^8.7.3",
        "reflect-metadata": "^0.1.13",
        "ts-node": "^10.2.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.4",
        "@types/faker": "^5.5.8",
        "@types/jest": "^27.0.3",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/node": "^17.0.4",
        "@types/pg": "^8.6.1",
        "@types/supertest": "^2.0.11",
        "jest": "^27.4.5",
        "nodemon": "^2.0.12",
        "prettier": "2.3.2",
        "supertest": "^6.1.5",
        "ts-jest": "^27.1.2",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.3.5"
    }
}
